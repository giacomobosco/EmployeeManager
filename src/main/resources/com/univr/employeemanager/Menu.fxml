<VBox prefHeight="600.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.univr.employeemanager.MenuController">
    <children>
        <MenuBar VBox.vgrow="NEVER">
            <menus>
                <Menu mnemonicParsing="false" text="File">
                    <items>
                        <MenuItem mnemonicParsing="false" text="New" />
                        <MenuItem mnemonicParsing="false" text="Open…" />
                        <Menu mnemonicParsing="false" text="Open Recent" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Close" />
                        <MenuItem mnemonicParsing="false" text="Save" />
                        <MenuItem mnemonicParsing="false" text="Save As…" />
                        <MenuItem mnemonicParsing="false" text="Revert" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Preferences…" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Quit" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                    <items>
                        <MenuItem mnemonicParsing="false" text="Undo" />
                        <MenuItem mnemonicParsing="false" text="Redo" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Cut" />
                        <MenuItem mnemonicParsing="false" text="Copy" />
                        <MenuItem mnemonicParsing="false" text="Paste" />
                        <MenuItem mnemonicParsing="false" text="Delete" />
                        <SeparatorMenuItem mnemonicParsing="false" />
                        <MenuItem mnemonicParsing="false" text="Select All" />
                        <MenuItem mnemonicParsing="false" text="Unselect All" />
                    </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                    <items>
                        <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
                    </items>
                </Menu>
            </menus>
        </MenuBar>
        <SplitPane dividerPositions="0.2505567928730512, 0.7505567928730512" focusTraversable="true" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
            <items>
                <AnchorPane>
               <children>
                  <RadioButton fx:id="ORenable" layoutX="24.0" layoutY="46.0" mnemonicParsing="false" onAction="#ORbuttonPress" text="OR" />
                  <RadioButton fx:id="ANDenable" layoutX="24.0" layoutY="14.0" mnemonicParsing="false" onAction="#ANDbuttonPress" text="AND" />
                  <CheckBox fx:id="hasLicenseEnable" layoutX="26.0" layoutY="91.0" mnemonicParsing="false" onAction="#hasLicenseEnablePress" text="Has License" />
                  <CheckBox fx:id="hasCarEnable" layoutX="27.0" layoutY="126.0" mnemonicParsing="false" onAction="#hasCarEnablePress" text="Has Car" />
                  <DatePicker fx:id="birthFromDate" layoutX="47.0" layoutY="165.0" onAction="#birthFromDatePress" prefHeight="25.0" prefWidth="158.0" promptText="birth from" />
                  <CheckBox fx:id="birthIntervalEnable" layoutX="19.0" layoutY="188.0" mnemonicParsing="false" onAction="#birthIntervalEnablePress" />
                  <DatePicker fx:id="birthToDate" layoutX="47.0" layoutY="202.0" onAction="#birthToDatePress" prefHeight="25.0" prefWidth="158.0" promptText="birth to" />
                  <Button fx:id="searchButton" layoutX="22.0" layoutY="490.0" mnemonicParsing="false" onAction="#searchButtonPress" style="-fx-background-color: green;" text="search" textAlignment="CENTER" textFill="WHITE">
                     <font>
                        <Font name="System Bold" size="13.0" />
                     </font>
                  </Button>
                  <Button fx:id="restoreButton" layoutX="138.0" layoutY="490.0" mnemonicParsing="false" onAction="#restoreButtonPress" text="restore">
                     <font>
                        <Font size="13.0" />
                     </font>
                  </Button>
                  <DatePicker fx:id="periodFromDate" layoutX="47.0" layoutY="249.0" onAction="#periodFromDatePress" prefHeight="25.0" prefWidth="158.0" promptText="period from" />
                  <DatePicker fx:id="periodToDate" layoutX="47.0" layoutY="289.0" onAction="#periodToDatePress" prefHeight="25.0" prefWidth="158.0" promptText="period to" />
                  <CheckBox fx:id="periodIntervalEnable" layoutX="19.0" layoutY="274.0" mnemonicParsing="false" onAction="#periodIntervalEnablePress" />
               </children>
                </AnchorPane>
                <ScrollPane prefHeight="-1.0" prefWidth="-1.0">
                    <content>
                        <AnchorPane id="Content" minHeight="-1.0" minWidth="-1.0" prefHeight="545.0" prefWidth="430.0">
                            <children>
                                <TableView fx:id="mainTable" layoutX="6.0" layoutY="-2.0" prefHeight="546.0" prefWidth="443.0">
                                    <columns>
                                        <TableColumn fx:id="nameField" prefWidth="75.0" text="Name" />
                                        <TableColumn fx:id="lastNameField" prefWidth="75.0" text="Last Name" />
                                        <TableColumn fx:id="birthDateField" prefWidth="75.0" text="Birth Date" />
                                        <TableColumn fx:id="addressField" prefWidth="75.0" text="Address" />
                                        <TableColumn fx:id="cellNumberField" prefWidth="75.0" text="Cell Number" />
                                    </columns>
                                </TableView>
                            </children>
                        </AnchorPane>
                    </content>
                </ScrollPane>
                <AnchorPane>
                    <children>
                        <Button fx:id="newButton" layoutX="63.0" layoutY="37.0" mnemonicParsing="false" onAction="#newButtonPress" prefHeight="27.0" prefWidth="95.0" text="New" />
                        <Button fx:id="editButton" disable="true" layoutX="62.0" layoutY="87.0" mnemonicParsing="false" onAction="#editButtonPress" prefHeight="27.0" prefWidth="95.0" text="Edit" />
                        <Button fx:id="deleteButton" disable="true" layoutX="63.0" layoutY="142.0" mnemonicParsing="false" onAction="#deleteButtonPress" prefHeight="27.0" prefWidth="95.0" text="Delete" />
                        <Button fx:id="detailsButton" disable="true" layoutX="62.0" layoutY="195.0" mnemonicParsing="false" onAction="#detailsButtonPress" text="Show details" />
                    </children>
                </AnchorPane>
            </items>
        </SplitPane>
        <HBox id="HBox" alignment="CENTER_LEFT" spacing="5.0" VBox.vgrow="NEVER">
            <children>
                <Label maxHeight="1.7976931348623157E308" maxWidth="-1.0" text="Left status" HBox.hgrow="ALWAYS">
                    <font>
                        <Font size="11.0" fx:id="x3" />
                    </font>
                    <textFill>
                        <Color red="0.625" green="0.625" blue="0.625" fx:id="x4" />
                    </textFill>
                </Label>
                <Pane prefHeight="-1.0" prefWidth="-1.0" HBox.hgrow="ALWAYS" />
                <Label font="$x3" maxWidth="-1.0" text="Right status" textFill="$x4" HBox.hgrow="NEVER" />
            </children>
            <padding>
                <Insets bottom="3.0" left="3.0" right="3.0" top="3.0" />
            </padding>
        </HBox>
    </children>
</VBox>
